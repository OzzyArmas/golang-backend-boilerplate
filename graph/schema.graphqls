# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  user_id: ID!
  user_name: String!
  phone_number: String!
}


type Query {
  getUser(user_id: ID): User!
}

input CreateUserInput {
  user_name: String!
  phone_number: String!
}

type Mutation {
  createUser(input: CreateUserInput): ID!
}
